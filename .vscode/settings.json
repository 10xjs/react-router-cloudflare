{
  "files.associations": {
    "wrangler.json": "jsonc",
    ".dev.vars": "properties",
    ".dev.vars.*": "properties",
    "*.css": "tailwindcss"
  },
  "editor.defaultFormatter": "biomejs.biome",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit"
  },
  "typescript.tsdk": "node_modules/typescript/lib"
}
